<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>

<AnchorPane xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.example.projeto2.controllers.ExcluirConsultaController"
            prefWidth="400" prefHeight="200"
            style="-fx-font-family: 'Arial'; -fx-padding: 20;">

    <VBox spacing="20" alignment="CENTER">
        <Label text="Excluir Consulta" style="-fx-font-size: 20px; -fx-font-weight: bold;" />
        <TextField fx:id="consultaIdField" promptText="ID da Consulta a excluir" />
        <Button text="Excluir" onAction="#handleExcluirConsulta" />
    </VBox>
</AnchorPane>
